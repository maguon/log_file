{
  "swagger": "2.0",
  "info": {
    "description": "The documents for myxxjs upload and download image or file api",
    "version": "1.0.0",
    "title": "Logistic file api doc",
    "contact": {
      "email": "info@myxxjs.com"
    },
    "license": {
      "name": "View source at github",
      "url": "https://github.com/maguon/log_file"
    }
  },
  "host": "stg.myxxjs.com:9002",
  "basePath": "/api",
  "tags": [
    {
      "name": "image",
      "description": "download and upload image files ",
      "externalDocs": {
        "description": " mimetype file api"
      }
    },
    {
      "name": "file",
      "description": "download and upload common files ",
      "externalDocs": {
        "description": " common file api"
      }
    }
  ],
  "schemes": [
    "http"
  ],
  "paths": {
    "/image/{imageId}": {
      "get": {
        "tags": [
          "image"
        ],
        "summary": "get image file",
        "description": "web or mobile device load image ",
        "operationId": "getImage",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "imageId",
            "in": "path",
            "description": "The image unique id",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "default": {
            "description": "successful operation"
          }
        }
      }
    },
    "/user/{userId}/image": {
      "post": {
        "tags": [
          "image"
        ],
        "summary": "uploads an image",
        "description": "imageType(1:DriverLic,2:TruckLic,3:Insurance)",
        "operationId": "uploadFile",
        "consumes": [
          "multipart/form-data"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "userId",
            "in": "path",
            "description": "user unique id",
            "required": true,
            "type": "integer"
          },
          {
            "name": "imageType",
            "in": "query",
            "description": "image type (1:DriverLic,2:TruckLic,3:Insurance,4:Damage,5:TruckDamage ,6:Finance,7:transportBill,9.TruckCheck)",
            "required": true,
            "type": "integer"
          },
          {
            "name": "image",
            "in": "formData",
            "description": "file to upload",
            "required": true,
            "type": "file"
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation"
          }
        }
      }
    },
    "/user/{userId}/file": {
      "post": {
        "tags": [
          "file"
        ],
        "summary": "uploads a common file",
        "description": "fileType(1:planning,2:car_fee)",
        "operationId": "uploadFile",
        "consumes": [
          "multipart/form-data"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "userId",
            "in": "path",
            "description": "user unique id",
            "required": true,
            "type": "integer"
          },
          {
            "name": "fileType",
            "in": "query",
            "description": "file type (1:planing,2:car_fee)",
            "required": true,
            "type": "integer"
          },
          {
            "name": "userType",
            "in": "query",
            "description": "user type",
            "required": true,
            "type": "integer"
          },
          {
            "name": "file",
            "in": "formData",
            "description": "file to upload",
            "required": true,
            "type": "file"
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation"
          }
        }
      },
      "get": {
        "tags": [
          "file"
        ],
        "summary": "get common file",
        "description": "get file id list by params ",
        "operationId": "getFileList",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "userId",
            "in": "path",
            "description": "The user unique id",
            "required": true,
            "type": "integer"
          },
          {
            "name": "userType",
            "in": "query",
            "description": "The user type",
            "required": false,
            "type": "string"
          },
          {
            "name": "fileType",
            "in": "query",
            "description": "The file type",
            "required": false,
            "type": "string"
          },
          {
            "name": "videoType",
            "in": "query",
            "description": "The video type",
            "required": false,
            "type": "string"
          },
          {
            "name": "fileId",
            "in": "query",
            "description": "The file id",
            "required": false,
            "type": "string"
          },
          {
            "name": "startDate",
            "in": "query",
            "description": "The file upload start date",
            "required": false,
            "type": "date"
          },
          {
            "name": "endDate",
            "in": "query",
            "description": "The file upload end date",
            "required": false,
            "type": "date"
          },
          {
            "name": "start",
            "in": "query",
            "description": "the start number for page",
            "required": false,
            "type": "integer"
          },
          {
            "name": "size",
            "in": "query",
            "description": "the size number for page",
            "required": false,
            "type": "integer"
          }
        ],
        "responses": {
          "default": {
            "description": "successful operation"
          }
        }
      }
    },
    "/user/{userId}/file/{fileId}": {
      "get": {
        "tags": [
          "file"
        ],
        "summary": "download  file",
        "description": "web or mobile device download file ",
        "operationId": "getFile",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "userId",
            "in": "path",
            "description": "The user unique id",
            "required": true,
            "type": "integer"
          },
          {
            "name": "fileId",
            "in": "path",
            "description": "The file unique id",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "default": {
            "description": "successful operation"
          }
        }
      }
    },
    "/user/{userId}/video": {
      "post": {
        "tags": [
          "video"
        ],
        "summary": "uploads a video",
        "description": "video(1:car)",
        "operationId": "uploadVideo",
        "consumes": [
          "multipart/form-data"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "userId",
            "in": "path",
            "description": "user unique id",
            "required": true,
            "type": "integer"
          },
          {
            "name": "videoType",
            "in": "query",
            "description": "video type (1:car)",
            "required": true,
            "type": "integer"
          },
          {
            "name": "userType",
            "in": "query",
            "description": "user type",
            "required": true,
            "type": "integer"
          },
          {
            "name": "file",
            "in": "formData",
            "description": "video to upload",
            "required": true,
            "type": "file"
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation"
          }
        }
      }
    },
    "/file/{fileId}/video.mp4": {
      "get": {
        "tags": [
          "video"
        ],
        "summary": "get video",
        "description": "web or mobile device get video",
        "operationId": "getVideo",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "userId",
            "in": "path",
            "description": "The user unique id",
            "required": true,
            "type": "integer"
          },
          {
            "name": "fileId",
            "in": "path",
            "description": "The file unique id",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "default": {
            "description": "successful operation"
          }
        }
      }
    }
  },
  "definitions": {
    "ImageMeta" :  {
      "type": "object",
      "required": [
        "imageType"
      ],
      "properties": {
        "imageType": {
          "type": "integer"
        }
      }
    }
  }

}